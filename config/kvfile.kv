<MobifySell>
    FloatLayout:
        ScrollView:
            id: scroll_home
            size_hint: 1, 0.88
            pos_hint: {'center_y': .44}
            bar_width: 10
            scroll_wheel_distance: 40
            scroll_type: ['bars', 'content']
            effect_cls: ScrollEffect
            BoxLayout:
                id: content
                orientation: 'vertical'
                size_hint: 1, None
                height: self.minimum_height
                padding: 22, 0, 22, 10
                canvas:
                    Color:
                        rgba: .15, .15, .15, .9
                    Rectangle:
                        size: self.size
                        pos: self.pos
        MenuHeader
    Sellwindow

<MobifyRepair>
    FloatLayout:
        ScrollView:
            id: scroll_home
            size_hint: 1, 0.88
            pos_hint: {'center_y': .44}
            bar_width: 10
            scroll_wheel_distance: 40
            scroll_type: ['bars', 'content']
            effect_cls: ScrollEffect
            BoxLayout:
                id: content
                orientation: 'vertical'
                size_hint: 1, None
                height: self.minimum_height
                padding: 22, 0, 22, 10
                canvas:
                    Color:
                        rgba: .15, .15, .15, .9
                    Rectangle:
                        size: self.size
                        pos: self.pos
        RepairWindow
        MenuHeader


<Sellwindow>
    BoxLayout:
        padding: self.width*0.01, self.height*0.2, self.width*0.01, self.height*0.01   #left, top, right, bottom
        FloatLayout:
            canvas:
                Color:
                    rgba: 1, 0.3, 0.2, 1
                Rectangle:
                    size: self.size
                    pos: self.pos
            Label:
                text: 'Sell Device'
                font_name: app.netflix_font
                font_size: 38
                size_hint: None, None
                size: self.texture_size[0], 130
                pos_hint: {'x': 0.020, 'y': 0.82}

            Label:
                text: 'Select Date'
                font_name: app.netflix_font
                font_size: 38
                size_hint: None, None
                size: self.texture_size[0], 130
                pos_hint: {'x': 0.30, 'y': 0.82}

            Spinner:
                id: brand
                size_hint: None, None
                size: 120, 44
                pos_hint: {'x': 0.020, 'y': 0.80}
                text: 'Choose Brand'
                values: 'OnePlus', 'Apple', 'Samsung'
                on_text:
                    root.price(self.values)

            Spinner:
                id: month
                size_hint: None, None
                size: 120, 44
                pos_hint: {'x': 0.30, 'y': 0.80}
                text: 'Choose Month'
                values: 'Oct', 'Nov', 'Dec'
                on_text:
                    root.pickup(self.values)

            Spinner:
                id: damage
                size_hint: None, None
                size: 120, 44
                pos_hint: {'x': 0.020, 'y': 0.67}
                text: 'Damage'
                values: 'Flawless', 'Dents', 'Not Working'
                on_text:
                    root.price(self.values)

            Spinner:
                id: day
                size_hint: None, None
                size: 120, 44
                pos_hint: {'x': 0.30, 'y': 0.67}
                text: 'Choose Date'
                values: '1','5','10','15','20','25','30'
                on_text:
                    root.pickup(self.values)

            Spinner:
                id: years
                size_hint: None, None
                size: 120, 44
                pos_hint: {'x': 0.020, 'y': 0.55}
                text: 'Device age'
                values: '1', '3', '5'
                on_text:
                    root.price(self.values)

            Spinner:
                id: warrant
                size_hint: None, None
                size: 120, 44
                pos_hint: {'x': 0.020, 'y': 0.42}
                text: 'Under Warranty?'
                values: 'Yes', 'No'
                on_text:
                    root.price(self.values)

            Button:
                size_hint: None,None
                size: 120,40
                pos_hint: {'x': 0.30, 'y': 0.55}
                background_color: 0,0,0,1
                background_normal: app.roundborder
                background_down: app.roundborder
                text: 'Confirm Order'
                bold: True
                on_press: root.display()


            Label:
                text: 'Order Summary'
                font_name: app.netflix_font
                font_size: 38
                color: 0,0,0,1
                size_hint: None, None
                size: self.texture_size[0], 130
                pos_hint: {'x': 0.60, 'y': 0.82}

            Label:
                id: brandchoosen
                text: ''
                font_name: app.netflix_font
                font_size: 28
                size_hint: None, None
                size: self.texture_size[0], 130
                pos_hint: {'x': 0.60, 'y': 0.72}

            Label:
                id: damagedone
                text: ''
                font_name: app.netflix_font
                font_size: 28
                size_hint: None, None
                size: self.texture_size[0], 130
                pos_hint: {'x': 0.750, 'y': 0.72}

            Label:
                id: yearsold
                text: ''
                font_name: app.netflix_font
                font_size: 28
                size_hint: None, None
                size: self.texture_size[0], 130
                pos_hint: {'x': 0.60, 'y': 0.62}

            Label:
                id: warranty
                text: ''
                font_name: app.netflix_font
                font_size: 28
                size_hint: None, None
                size: self.texture_size[0], 130
                pos_hint: {'x': 0.750, 'y': 0.62}

            Label:
                text: 'Pickup Date'
                font_name: app.netflix_font
                font_size: 38
                size_hint: None, None
                color: 0,0,0,1
                size: self.texture_size[0], 130
                pos_hint: {'x': 0.60, 'y': 0.52}

            Label:
                id: date
                text: ''
                font_name: app.netflix_font
                font_size: 28
                size_hint: None, None
                size: self.texture_size[0], 130
                pos_hint: {'x': 0.600, 'y': 0.42}

            Label:
                text: 'Resale Value'
                font_name: app.netflix_font
                color: 0,0,0,1
                font_size: 38
                size_hint: None, None
                size: self.texture_size[0], 130
                pos_hint: {'x': 0.60, 'y': 0.32}
            Label:
                id:resale
                text: ''
                font_name: app.netflix_font
                font_size: 28
                color: 0,0,0,1
                size_hint: None, None
                size: self.texture_size[0], 130
                pos_hint: {'x': 0.60, 'y': 0.22}

