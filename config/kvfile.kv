#############################################################################
<CustomButton@Button>:
    size_hint: None, 1
    width: self.texture_size[0] + 20
    background_color: 0, 0, 0, 0
    on_press: print(self.text)
	
<ContentList>:
    orientation: 'vertical'
    size_hint: 1, None
    height: self.minimum_height
    Label:
        size_hint: None, None
        size: self.texture_size
        id: label
        bold: True
        font_size: 25
        color: .9, .9, .9, 1
    ScrollView:
        size_hint: 1, None 
        height: 150
        do_scroll: True, True
        effect_cls: ScrollEffect
		bar_color: 1, 1, 1, 0
		bar_inactive_color: 1, 1, 1, 0
        scroll_type: ['content']
        GridLayout:
            id: grid
            size_hint: None, None
            size: self.minimum_width, 151
            spacing: 7

<SearchBox>:
    Button:
        id: searchbtn
        size_hint: .08, .35
        pos_hint: {'center_y': .5, 'center_x': .9}
        background_normal: app.search_btn
        background_down: app.search_btn
        border: 0, 0, 0, 0

<MenuHeader@BoxLayout>:
	size_hint: 1, 0.12
	pos_hint: {'top': 1}
	canvas:
		Color:
			rgba: .15, .15, .15, .9
		Rectangle:
			size: self.size
			pos: self.pos
	Image:
		size_hint: None, 1.3
		width: 170
		pos_hint: {'center_y': .55}
		source: app.netflix_logo
	CustomButton:
		text: 'Sell'
		bold: True
		on_press: app.root.current = 'mobifysell'
	CustomButton:
		text: 'Repair'
		bold: True
		on_press: app.root.current= 'mobifyrepair'
	CustomButton:
		text: 'Reviews'
		bold: True
        on_press: app.root.current ='netflixmenu'
    CustomButton:
        text: 'My Orders'
        bold: True


    SearchBox

	FloatLayout:
	    size_hint: .3, 1
	    drop_menu: drop_menu.__self__
		# DropdownMenu
        Button:
            size_hint: .5, .55
            pos_hint: {'center_y': .5, 'center_x': .5}
            on_parent: drop_menu.dismiss()
            on_release: drop_menu.open(self)
            Image:
                source: app.dropmenu_icon
                x: self.parent.x
                y: self.parent.y
                size_hint: None, None
                size: 40, 40
        DropDown:
            id: drop_menu
            pos_hint: {'center_x': .86}
            auto_width: False
            width: 170
            on_select: print(args[1])
            Label:
                size_hint: 1, None
                height: self.texture_size[1]
                text: ' '*36 + '^'
                halign: 'left'
            BoxLayout:
                orientation: 'vertical'
                size_hint: None, None
                width: 170
                height: 200
                canvas.before:
                    Color:
                        rgba: 0, 0, 0, .8
                    Rectangle:
                        size: self.size
                        pos: self.pos
                BoxLayout:
                    orientation: 'vertical'
                    padding: 10, 10, 10, 10
                    BoxLayout:
                        Button:
                            size_hint: None, None
                            size: 37, 37
                            pos_hint: {'top': 1}
                            on_press: drop_menu.select('Usuário: ' + btn_user.text)
                            Image:
                                source: app.dropmenu_icon
                                x: self.parent.x
                                y: self.parent.y
                                size_hint: None, None
                                size: self.parent.size
                        Button:
                            id: btn_user
                            pos_hint: {'center_y': .6}
                            background_color: 0, 0, 0, 0
                            text: 'Logged In'
                            on_press: drop_menu.select('Usuário: ' + self.text)
                    Button:
                        size_hint: 1, .5
                        background_color: 1, 1, 1, 0
                        text: 'MENU'
                        on_press: drop_menu.select('Opção: ' + self.text)
                BoxLayout:
                    orientation: 'vertical'
                    padding: 10, 0, 10, 5
                    Button:
                        background_color: 1, 1, 1, 0
                        bold: True
                        text: 'Repair'
                        text_size: self.size
                        halign: 'left'
                        valign: 'center'
                        on_press: app.root.current ='mobifyrepair'

                    Button:
                        background_color: 1, 1, 1, 0
                        bold: True
                        text: 'Centro de Ajuda'
                        text_size: self.size
                        halign: 'left'
                        valign: 'center'
                        on_press: drop_menu.select('Opção: ' + self.text)
                    Button:
                        background_color: 1, 1, 1, 0
                        bold: True
                        text: 'Logout'
                        text_size: self.size
                        halign: 'left'
                        valign: 'center'
                        on_release:
                            drop_menu.dismiss()
                            app.root.current = 'loginscreen'
                            app.root.get_screen('loginscreen').ids.email.focus = True
        # End of the DropDown Menu

<Banner>:
    size_hint: 1.056, None
    height: 450
    pos_hint: {'center_x': .5}
    FloatLayout:
        pos: self.parent.pos
        Image:
            source: app.banner
            size_hint:[1, None]
            height: 550
            pos_hint: {'top': 1}
            allow_stretch:True
            keep_ratio:False
        Label:
            text: 'Downey Jr.\nwith the\n 8 Pro'
            font_name: app.netflix_font
            bold: True
            font_size: 50
            pos_hint: {'center_y': .7, 'center_x': .15}
            color: 1, 1, 1, 1
        Button:
            size_hint: None, None
            size: 140, 50
