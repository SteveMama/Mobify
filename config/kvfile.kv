#:import ScrollEffect kivy.effects.scroll.ScrollEffect
#:import FadeTransition kivy.uix.screenmanager.FadeTransition

<Manager>:
    transition: FadeTransition()
    LoginScreen:
        name: 'loginscreen'
    NetflixHome:
        name: 'netflixmenu'
    ResultPage:
        name: 'resultpage'
    AccountPage:
        name: 'accountpage'
    MobifyRepair:
        name: 'mobifyrepair'
    MobifySell:
        name: 'mobifysell'

########################################################################################################################
<UserInput@TextInput>:
    size_hint: None, None
    size: 280, 40
    font_size: 20
    multiline: False
    background_color: .2, .2, .2, 1
    foreground_color: 1, 1, 1, 1
    background_normal: app.roundborder
    background_active: app.roundborder


<AccountPage>
    canvas:
        Color:
            rgba: 1, 1, 1, 0.5
        Rectangle:
            source: app.background_login
            size: self.size
            pos: self.pos
    BoxLayout:
        size_hint: 1, 0.12
        pos_hint: {'top': 1}
        Image:
            source: app.netflix_logo
            pos_hint: {'center_y': .5}
            size_hint: None, None
            size: 170, 90
    BoxLayout:
        padding: self.width*0.3, self.height*0.12, self.width*0.3, self.height*0.12   #left, top, right, bottom
        FloatLayout:
            canvas:
                Color:
                    rgba: 0, 0, 0, 0.6
                Rectangle:
                    size: self.size
                    pos: self.pos
            Label:
                text: 'Create Account'
                font_name: app.netflix_font
                font_size: 45
                size_hint: None, None
                size: self.texture_size[0], 130
                pos_hint: {'x': 0.075, 'center_y': .90}
            UserInput:
                id: name
                pos_hint:{'center_x': 0.5, 'center_y': 0.78}
                focus: True
                hint_text:'Full Name'
                write_tab: False
                on_text_validate:
            UserInput:
                id: email
                pos_hint: {'center_x': 0.5, 'center_y': .68}
                focus: True
                hint_text: 'Email'
                write_tab: False
                on_text_validate: root.login_verification()
            UserInput:
                id: passw
                pos_hint: {'center_x': 0.5, 'center_y': .58}
                hint_text: 'Password'
                password: True
                on_text: root.istab_pressed(self.text)
                on_text_validate: root.login_verification()
            UserInput:
                id: number
                pos_hint:{'center_x':0.5, 'center_y': 0.48}
                hint_text: 'Ph. Number'
                write_tab: False
                on_text_validate:

            UserInput:
                id: address
                pos_hint:{'center_x':0.5, 'center_y': 0.38}
                hint_text: 'Address'
                write_tab: False
                on_text_validate:

            Button:
                size_hint: None,None
                size: 280,40
                pos_hint: {'center_x':0.5, 'center_y': .28 }
                background_color: 1,0.3,0.2,1
                background_normal: app.roundborder
                background_down: app.roundborder
                text: 'Create Account'
                bold: True
                on_press: root.openaccountpage()

            Label:
                size_hint: None, None
                size: self.texture_size
                pos_hint: {'center_x': 0.35, 'center_y': .19}
                text: ''
            Label:
				id: dica
                pos_hint: {'center_x': 0.5, 'center_y': .05}

            Button:
                size_hint: None,None
                size: 280,40
                pos_hint: {'center_x':0.5, 'center_y': .10 }
                background_color: 1,0.3,0.1,1
                background_normal: app.roundborder
                background_down: app.roundborder
                text: 'Login Now'
                bold: True
                on_press: root.openloginpage()

<PopupWarning>
    size_hint: None, None
    size: 232, 200
    title: 'Incorrect creds/no account found'
    separator_color: 1, 0.3, 0.2, 1
    BoxLayout:
        orientation: 'vertical'
        Label:
            size_hint: 1, .7
            text_size: self.size
        Button:
            pos_hint: {'center_x': .5}
            size_hint: .5, .3
            text: 'Ok'
            on_press: root.dismiss()

<LoginScreen>:
    canvas:
        Color:
            rgba: 1, 1, 1, 0.5
        Rectangle:
            source: app.background_login
            size: self.size
            pos: self.pos
    BoxLayout:
        size_hint: 1, 0.12
        pos_hint: {'top': 1}
        Image:
            source: app.netflix_logo
            pos_hint: {'center_y': .5}
            size_hint: None, None
            size: 170, 90
    BoxLayout:
        padding: self.width*0.3, self.height*0.12, self.width*0.3, self.height*0.12   #left, top, right, bottom
        FloatLayout:
            canvas:
                Color:
                    rgba: 0, 0, 0, 0.6
                Rectangle:
                    size: self.size
                    pos: self.pos
            Label:
                text: 'Login/Signup \nto Sell'
                font_name: app.netflix_font
                font_size: 45
                size_hint: None, None
                size: self.texture_size[0], 130
                pos_hint: {'x': 0.075, 'center_y': .85}
            UserInput:
                id: email
                pos_hint: {'center_x': 0.5, 'center_y': .6}
                focus: True
                hint_text: 'Email'
                write_tab: False
                on_text_validate: root.login_verification()
            UserInput:
                id: passw
                pos_hint: {'center_x': 0.5, 'center_y': .47}
                hint_text: 'Password'
                password: True
                on_text: root.istab_pressed(self.text)
                on_text_validate: root.login_verification()
            Button:
                size_hint: None, None
                size: 280, 40
                pos_hint: {'center_x': 0.5, 'center_y': .28}
                background_color: 1, 0.3, 0.2, 1
                background_normal: app.roundborder
                background_down: app.roundborder
                text: 'Login'
                bold: True
                on_press: root.login_verification()

            Button:
                size_hint: None,None
                size: 280,40
                pos_hint: {'center_x':0.5, 'center_y': .10 }
                background_color: 1,0.3,0.2,1
                background_normal: app.roundborder
                background_down: app.roundborder
                text: 'Create Account'
                bold: True
                on_press: root.openaccountpage()

            CheckBox:
                id: checkbox
                size_hint: None, None
                size: 25, 25
                pos_hint: {'center_x': 0.1, 'center_y': .19}
                canvas.before:
                    Color:
                        rgba: .3, .3, .3, 1
                    Rectangle:
                        size: self.size[0] - 10, self.size[1] - 10
                        pos: self.pos[0] + 5, self.pos[1] + 5
                color: 0, 0, 0, 1
            Label:
                size_hint: None, None
                size: self.texture_size
                pos_hint: {'center_x': 0.35, 'center_y': .19}
                text: 'Remember me'
            Label:
				id: dica
                pos_hint: {'center_x': 0.5, 'center_y': .05}
